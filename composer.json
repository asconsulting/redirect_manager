{
    "name":"asconsulting/redirect_manager",
    "description":"URL redirection module for Contao 4.9+",
    "homepage": "https://andrewstevens.consulting",
    "keywords":["contao", "htaccess", ".htaccess", "redirect", "rewrite", "url", "301", "302"],
    "type":"contao-module",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Andrew Stevens",
            "homepage":"https://andrewstevens.consulting"
        }
    ],
    "require":{
        "php": ">=7.4",
        "contao/core-bundle": ">=4.13",
        "contao-community-alliance/composer-plugin": "~2.4 || ~3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "RedirectManager\\": [
                "system/modules/redirect_manager/library/RedirectManager"
            ]
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "system/modules/redirect_manager": "system/modules/redirect_manager"
            }
        }
    },
    "minimum-stability": "dev"
}